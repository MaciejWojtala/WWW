              td.tabela_najlepsze_wyniki_kolumna2= _result.bests[1][4]
              td.tabela_najlepsze_wyniki_kolumna2= _result.avg[1]
            tr#tabela_najlepsze_wyniki_wiersz3
              td.tabela_najlepsze_wyniki_kolumna1
